{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "CodeFactoryVisualStudio/CodeFactory/CodeFactory.csproj",
            "CodeFactoryVisualStudio/CodeFactory.DotNet/CodeFactory.DotNet.csproj",
            "CodeFactoryVisualStudio/CodeFactory.Formatting.CSharp/CodeFactory.Formatting.CSharp.csproj",
            "CodeFactoryVisualStudio/CodeFactory.Logging/CodeFactory.Logging.csproj",
            "CodeFactoryVisualStudio/CodeFactory.VisualStudio/CodeFactory.VisualStudio.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "CodeFactory Platform",
      "_appName": "CodeFactory Platform",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.png",
      "_copyrightFooter": "Â© CodeFactory, LLC. All rights reserved.",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "template": [ "default", "templates/singulinkfx", "plugins/memberpage.2.59.0/content" ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "gettingstarted/**.md",
          "gettingstarted/**/toc.yml",
          "guidance/**.md",
          "guidance/**/toc.yml",
          "releasenotes/**.md",
          "releasenotes/**/toc.yml",
          "toc.yml",
          "*.md",
          "CNAME*.*"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}